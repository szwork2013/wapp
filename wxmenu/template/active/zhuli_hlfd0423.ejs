$(function(){

    //定时器更新时间
    var updateLastTime = function(){
        var now = moment()
        var days = endTime.diff(now, 'days')
        var hours = endTime.diff(now, 'hours') - days*24
        var minutes = endTime.diff(now, 'minutes') - days*24*60 - hours*60
        if(minutes<0){
            $('#last_day').html(0)
            $('#last_hour').html(0)
            $('#last_minute').html(0)
        }
        else{
             $('#last_day').html(days)
            $('#last_hour').html(hours)
