            //如果已经领过奖了
            if(myPrizeList.length >= activeObj.prizeAmount){
                return false;
            }
            else{
                window.selPrizeId = false
                that.removeClass('selected')
                return false;
            }
        }

        prizeUl.removeClass('selected')
